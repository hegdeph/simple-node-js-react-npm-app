
    pipeline{
    agent none
    environment {
        CI = 'true'
    }
    stages {
        
        
        stage('Build') {
            agent{
                ecs {
                inheritFrom 'Jenkins-Cloud-Agent-NodeJs'
                cpu 1024
                memory 2048
                image 'node:6-alpine'
                
            }
            steps {
                sh 'npm install'
            }
            }
        }
        
    }
    }
